#include <iostream>
#include <set> // for storing unique elements
using namespace std;

int main() {
    int n;
    cout << "Enter number of elements: ";
    cin >> n;

    int arr[n];
    cout << "Enter array elements: ";
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    set<int> uniqueElements; // stores distinct values

    for (int i = 0; i < n; i++) {
        uniqueElements.insert(arr[i]);
    }

    cout << "Total distinct elements: " << uniqueElements.size() << endl;

    return 0;
}
