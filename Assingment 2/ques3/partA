#include <iostream>
using namespace std;

int findMissing(int arr[], int n) {
    int totalSum = (n * (n + 1)) / 2; // sum from 1 to n
    int arraySum = 0;

    for (int i = 0; i < n - 1; i++) {
        arraySum += arr[i];
    }

    return totalSum - arraySum;
}

int main() {
    int arr[] = {1, 2, 3, 5, 6}; // Example: Missing 4
    int n = 6; // Array should contain numbers from 1 to 6

    cout << "Missing Number: " << findMissing(arr, n) << endl;

    return 0;
}
